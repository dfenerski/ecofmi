<core:FragmentDefinition height="100%"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:x="fmi.Eco.lib.controls">
    <Dialog title="Log" draggable="true">
        <x:Video url="{local>url}" contentType="{local>contentType}"/>
        <customData>
            <core:CustomData key="logId" />
        </customData>
        <endButton>
            <Button text="Cancel" press="closeDialog"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>