<mvc:View controllerName="fmi.Eco.controller.Analytics"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page showHeader="false">
        <content>
            <HeaderContainer class="sapUiTinyMarginTop sapUiLargeMarginBottom">
                <!-- <SearchField enabled="false" class="sapUiSmallMarginTop" /> -->
                <VBox alignItems="End">
                    <NumericContent value="" valueColor="Neutral" />
                    <Text text="" />
                </VBox>
                <VBox alignItems="End">
                    <NumericContent value="" valueColor="Error" />
                    <Text text="" />
                </VBox>
                <VBox alignItems="End">
                    <NumericContent value="" valueColor="Good" />
                    <Text text="" />
                </VBox>
            </HeaderContainer>
            <ScrollContainer>
                <Carousel id="mainBoard" height="100%">
                    <customLayout>
                        <CarouselLayout visiblePagesCount="{= ${device>/system/phone}? 1 : 3 }" />
                    </customLayout>
                    <pages>
                    </pages>
                </Carousel>
            </ScrollContainer>
        </content>
    </Page>
</mvc:View>