{
  "_version": "1.12.0",
  "sap.app": {
    "id": "fmi.Eco",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.40.12"
    },
    "dataSources": {
      "local": {
        "uri": "model/local.json",
        "type": "JSON"
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "fmi.Eco.view.Root",
      "type": "XML",
      "async": true,
      "id": "Root"
    },
    "dependencies": {
      "minUI5Version": "1.65.6",
      "libs": {
        "sap.ui.layout": {},
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "local": {
        "dataSource": "local",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "fmi.Eco.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "fmi.Eco.view",
        "controlAggregation": "pages",
        "controlId": "rootNavigator",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteRoot",
          "pattern": "",
          "target": ["TargetHome"]
        },
        {
          "name": "RouteHome",
          "pattern": "home",
          "target": ["TargetHome"]
        },
        {
          "name": "RouteRanked",
          "pattern": "ranked",
          "target": ["TargetRanked"]
        },
        {
          "name": "RouteAnalytics",
          "pattern": "analytics",
          "target": ["TargetAnalytics"]
        },
        {
          "name": "RouteInfo",
          "pattern": "info",
          "target": ["TargetInfo"]
        },
        {
          "name": "RouteLogCheck",
          "pattern": "review",
          "target": ["TargetLogCheck"]
        },
        {
          "name": "RouteTipAdmin",
          "pattern": "tipadmin",
          "target": ["TargetTipAdmin"]
        }
      ],
      "targets": {
        "TargetRoot": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Root",
          "viewName": "Root"
        },
        "TargetHome": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Home",
          "viewName": "Home"
        },
        "TargetRanked": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Ranked",
          "viewName": "Ranked"
        },
        "TargetAnalytics": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Analytics",
          "viewName": "Analytics"
        },
        "TargetInfo": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Info",
          "viewName": "Info"
        },
        "TargetLogCheck": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "LogCheck",
          "viewName": "LogCheck"
        },
        "TargetTipAdmin": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "TipAdmin",
          "viewName": "TipAdmin"
        }
      }
    }
  }
}
